[
  {
    "name": "rails-form-training",
    "image": "477438465448.dkr.ecr.ap-northeast-1.amazonaws.com/rails-form-training/prd:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "nginx",
        "awslogs-group": "/ecs/rails-form-training"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ]
  }
]